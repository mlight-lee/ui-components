{
  "name": "@mlightcad/ui-components",
  "version": "0.0.16",
  "description": "One common UI component library based on Element Plus",
  "license": "MIT",
  "author": "MLight Lee <mlight.lee@outlook.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/mlight-lee/ui-components"
  },
  "keywords": [
    "ui",
    "component",
    "vue",
    "element plus",
    "cad",
    "AutoCAD",
    "tool palette",
    "mlight"
  ],
  "main": "dist/ui-components.umd.js",
  "module": "dist/ui-components.es.js",
  "type": "module", 
  "types": "./lib/index.d.ts",
  "files": [
    "dist",
    "lib/*.js",
    "lib/*.ts",
    "src",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "clean": "rimraf dist lib tsconfig.tsbuildinfo",
    "build": "vue-tsc && vite build",
    "generate": "vite-ssg build",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "rollup-plugin-peer-deps-external": "^2.2.4"
  },
  "peerDependencies": {
    "element-plus": "^2.8.1",
    "vue": "^3.4.21"
  }
}
